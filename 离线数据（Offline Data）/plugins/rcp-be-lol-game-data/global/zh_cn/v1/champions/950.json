    "spells": [
        {
            "spellKey": "q",
            "name": "暗裔犬牙",
            "abilityIconPath": "/lol-game-data/assets/ASSETS/Characters/Naafiri/HUD/Icons2D/Icons_Naafiri_Q.png",
            "abilityVideoPath": "champion-abilities/0950/ability_0950_Q1.webm",
